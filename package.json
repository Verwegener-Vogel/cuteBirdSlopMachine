{
  "name": "cutebirdslopmachine",
  "version": "1.0.0",
  "description": "An AI-powered video generation system that creates adorable bird content featuring Northern German Baltic coastal species.",
  "main": "index.js",
  "scripts": {
    "dev": "wrangler dev --env-file .env --port 8787",
    "deploy": "wrangler deploy",
    "generate-prompts": "wrangler dev --test-scheduled",
    "tail": "wrangler tail",
    "generate-api-key": "node scripts/generate-api-key.js",
    "generate-bruno": "node scripts/generate-bruno-collection.js",
    "setup": "node scripts/setup-wrangler.js",
    "setup:db": "node scripts/setup-local-db.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:bruno": "node test/bruno-generator.test.js",
    "test:videos": "vitest run test/videos-endpoint.test.js",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.9.3",
    "@cloudflare/workers-types": "^4.20250920.0",
    "@playwright/test": "^1.55.1",
    "@types/node": "^24.5.2",
    "@vitest/ui": "^3.2.4",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "miniflare": "^4.20250917.0",
    "playwright": "^1.55.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.38.0"
  }
}
